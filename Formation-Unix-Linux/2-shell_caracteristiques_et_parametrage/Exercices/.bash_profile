# Get the aliases and functions
[[ -f ~/.bashrc ]] && . ~/.bashrc

# User specific environment and startup programs
BASH_ENV=$HOME/.bashrc
USERNAME="ChocolateCharlie"

export USERNAME BASH_ENV

# Show greeting message
echo $USERNAME
uname -sm

